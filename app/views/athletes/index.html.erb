<h1>Listing athletes</h1>

<table>
  <tr>
    <th>Vorname</th>
    <th>Nachname</th>
    <th>Strasse</th>
    <th>Gemeinde</th>
    <th>Jahrgang</th>
    <th>Geschlecht</th>
    <th>Verein</th>
    <th>Lizenz</th>
    <th>Zeit</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @athletes.each do |athlete| %>
  <tr>
    <td><%= athlete.vorname %></td>
    <td><%= athlete.nachname %></td>
    <td><%= athlete.strasse %></td>
    <td><%= athlete.gemeinde %></td>
    <td><%= athlete.jahrgang %></td>
    <td><%= athlete.geschlecht %></td>
    <td><%= athlete.verein %></td>
    <td><%= athlete.lizenz %></td>
    <td><%= athlete.zeit %></td>
    <td><%= link_to 'Show', athlete %></td>
    <td><%= link_to 'Edit', edit_athlete_path(athlete) %></td>
    <td><%= link_to 'Destroy', athlete, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

